{
  "manifest_version": 3,
  "name": "CleansedFeedback",
  "version": "1.1",
  "description": "네이버 웹툰 댓글 페이지에서 비난을 순화한 정제된 피드백을 제공하는 서비스입니다.",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": [
    "https://comic.naver.com/webtoon/detail*",
    "http://localhost:5000/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images/logo2.png"
  },
  "icons": {
    "16": "images/logo2.png",
    "48": "images/logo2.png",
    "128": "images/logo2.png"
  }
}
